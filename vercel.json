{
    "version": 2,
    "builds": [
        {
            "src": "quotes/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/quotes/wsgi.py"
        }
    ],
    "functions": {
        "quotes/wsgi.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}
