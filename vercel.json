{
    "version": 2,
    "builds": [
        {
            "src": "api/vercel_handler.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/api/vercel_handler.py"
        }
    ]
}
